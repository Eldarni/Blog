---
layout: layout
title: Archive
---
<section class="archive">

    {% for post in collections.post|reverse %}
        <a class="alt" href="{{ post.url }}" title="{{ post.data.title }}">
            <h2>{{ post.data.title }}</h2>
            <time datetime="{{ post.date|timestamp }}">{{ post.date|date }}</time>
        </a>
    {% endfor %}

</section>